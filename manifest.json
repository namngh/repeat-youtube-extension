{
    "manifest_version": 3,
    "name": "Repeat Youtube",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html"
    },
    "description": "Choose a part of video in Youtube to repeat",
    "icons": {
        "16": "assets/icons/icon16.png",
        "32": "assets/icons/icon32.png",
        "48": "assets/icons/icon48.png",
        "128": "assets/icons/icon128.png"
    },
    "author": "namngh1202@gmail.com",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/watch?*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}